        <locationX>366</locationX>
        <locationY>294</locationY>
        <assignNextValueToReference>CurrentLoopRecord</assignNextValueToReference>
        <collectionReference>RecordTypeCollection</collectionReference>
        <iterationOrder>Asc</iterationOrder>
        <nextValueConnector>
            <targetReference>Record_Type</targetReference>
        </nextValueConnector>
        <noMoreValuesConnector>
            <targetReference>Record_Type_IDs_Assignment_Completed</targetReference>
        </noMoreValuesConnector>
    </loops>
